<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Material Dialog -->
    <style name="ThemeOverlay.App.MaterialAlertDialog" parent="ThemeOverlay.Material3.MaterialAlertDialog">
        <item name="colorPrimary">?attr/colorOnPrimary</item>
        <item name="colorOnSurface">?attr/colorSecondary</item>
        <item name="buttonBarPositiveButtonStyle">@style/Widget.App.Button.Dialog</item>
        <item name="buttonBarNegativeButtonStyle">@style/Widget.App.Button.Dialog</item>
        <item name="buttonBarNeutralButtonStyle">@style/Widget.App.Button.Dialog</item>
    </style>
    <style name="Widget.App.Button.Dialog" parent="Widget.Material3.Button.TextButton.Dialog">
        <item name="android:textColor">?attr/colorOnBackground</item>
    </style>

    <!-- Material Buttons -->
    <style name="Widget.App.Button.TextButton" parent="Widget.Material3.Button.TextButton">
        <item name="android:textColor">?attr/colorOnBackground</item>
    </style>
    <style name="Widget.App.Button.IconButton" parent="Widget.Material3.Button.IconButton">
        <item name="iconTint">?attr/colorOnBackground</item>
d    </style>
    <style name="Widget.App.Button.OutlinedButton" parent="Widget.Material3.Button.OutlinedButton.Icon">
        <item name="iconTint">?attr/colorPrimary</item>
        <item name="android:textColor">?attr/colorOnBackground</item>
d    </style>

    <style name="Widget.App.FloatingActionButton.Primary" parent="Widget.Material3.FloatingActionButton.Primary">
        <item name="backgroundTint">?attr/colorPrimary</item>
        <item name="tint">?attr/colorOnPrimary</item>
    </style>

</resources>